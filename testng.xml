<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Automation Group Test">
    <parameter name="config" value="single.conf.json"/>
    <test name="Parallel Test" thread-count="1" parallel="classes">
        <groups>
            <run>
                <include name="all" />
                <exclude name="regression" />
                <exclude name="sanity" />
            </run>
        </groups>
        <classes>
            <class name="testcases.TC13"/>
            <class name="testcases.TC14"/>
            <class name="testcases.TC15"/>
        </classes>

    </test>
</suite>
